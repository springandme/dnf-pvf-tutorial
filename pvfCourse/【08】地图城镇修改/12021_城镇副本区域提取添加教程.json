\t\t\t</td>\n\t\t</tr>\n\t</tbody>\n</table>\n\n<table style=\"width:100%\">\n\t<tbody>\n\t\t<tr>\n\t\t\t<td>\n\t\t\t<div>\n\t\t\t<p>&nbsp;</p>\n\t\t\t</div>\n\t\t\t</td>\n\t\t</tr>\n\t</tbody>\n</table>\n\n<table style=\"width:100%\">\n\t<tbody>\n\t\t<tr>\n\t\t\t<td>\n\t\t\t<div>\n\t\t\t<p>&nbsp;</p>\n\t\t\t</div>\n\t\t\t</td>\n\t\t</tr>\n\t</tbody>\n</table>\n\n<table style=\"width:100%\">\n\t<tbody>\n\t\t<tr>\n\t\t\t<td>\n\t\t\t<div>\n\t\t\t<p>&nbsp;</p>\n\t\t\t</div>\n\t\t\t</td>\n\t\t</tr>\n\t</tbody>\n</table>\n\n<table style=\"width:100%\">\n\t<tbody>\n\t\t<tr>\n\t\t\t<td>\n\t\t\t<div>\n\t\t\t<p>&nbsp;</p>\n\t\t\t</div>\n\t\t\t</td>\n\t\t</tr>\n\t</tbody>\n</table>\n\n<table style=\"width:100%\">\n\t<tbody>\n\t\t<tr>\n\t\t\t<td>\n\t\t\t<div>\n\t\t\t<p>&nbsp;</p>\n\t\t\t</div>\n\t\t\t</td>\n\t\t</tr>\n\t</tbody>\n</table>\n\n<table style=\"width:100%\">\n\t<tbody>\n\t\t<tr>\n\t\t\t<td>\n\t\t\t<div>\n\t\t\t<p>&nbsp;</p>\n\t\t\t</div>\n\t\t\t</td>\n\t\t</tr>\n\t</tbody>\n</table>\n\n<table style=\"width:100%\">\n\t<tbody>\n\t\t<tr>\n\t\t\t<td>\n\t\t\t<div>\n\t\t\t<p>&nbsp;</p>\n\t\t\t</div>\n\t\t\t</td>\n\t\t</tr>\n\t</tbody>\n</table>\n\n<table style=\"width:100%\">\n\t<tbody>\n\t\t<tr>\n\t\t\t<td>\n\t\t\t<div>\n\t\t\t<p>&nbsp;</p>\n\t\t\t</div>\n\t\t\t</td>\n\t\t</tr>\n\t</tbody>\n</table>\n\n<table style=\"width:100%\">\n\t<tbody>\n\t\t<tr>\n\t\t\t<td>\n\t\t\t<div>\n\t\t\t<p>&nbsp;</p>\n\t\t\t</div>\n\t\t\t</td>\n\t\t</tr>\n\t</tbody>\n</table>\n\n<table style=\"width:100%\">\n\t<tbody>\n\t\t<tr>\n\t\t\t<td>\n\t\t\t<div>\n\t\t\t<p>&nbsp;</p>\n\t\t\t</div>\n\t\t\t</td>\n\t\t</tr>\n\t</tbody>\n</table>\n\n<table style=\"width:100%\">\n\t<tbody>\n\t\t<tr>\n\t\t\t<td>\n\t\t\t<div>\n\t\t\t<p>&nbsp;</p>\n\t\t\t</div>\n\t\t\t</td>\n\t\t</tr>\n\t</tbody>\n</table>\n\n<table style=\"width:100%\">\n\t<tbody>\n\t\t<tr>\n\t\t\t<td>\n\t\t\t<div>\n\t\t\t<p><span style=\"font-size:10.5pt\"><span style=\"color:red\">去对应位置提取副本背景 ui</span></span></p>\n\t\t\t</div>\n\t\t\t</td>\n\t\t</tr>\n\t</tbody>\n</table>\n\n<table style=\"width:100%\">\n\t<tbody>\n\t\t<tr>\n\t\t\t<td>\n\t\t\t<div>\n\t\t\t<p>&nbsp;</p>\n\t\t\t</div>\n\t\t\t</td>\n\t\t</tr>\n\t</tbody>\n</table>\n\n<table style=\"width:100%\">\n\t<tbody>\n\t\t<tr>\n\t\t\t<td>\n\t\t\t<div>\n\t\t\t<p><span style=\"font-size:10.5pt\"><span style=\"color:red\">去对应位置提取副本选择 ui </span></span></p>\n\t\t\t</div>\n\t\t\t</td>\n\t\t</tr>\n\t</tbody>\n</table>\n\n<p>&nbsp;</p>\n\n<p style=\"margin-left:42px; text-align:justify\"><span style=\"font-size:14pt\"><span style=\"font-family:微软雅黑,sans-serif\"><span style=\"color:black\"><span style=\"color:#0070c0\">253681</span><span style=\"color:#0070c0\">中的（253，68）是副本坐标，用梦大的副本坐标图参考。 </span></span></span></span></p>\n\n<p style=\"margin-left:79px; text-align:justify\"><span style=\"font-size:14pt\"><span style=\"font-family:微软雅黑,sans-serif\"><span style=\"color:black\"><span style=\"color:#0070c0\">3126011</span><span style=\"color:#0070c0\">中的6011是副本代码。 </span></span></span></span></p>\n\n<p style=\"margin-left:42px; text-align:justify\"><span style=\"font-size:14pt\"><span style=\"font-family:微软雅黑,sans-serif\"><span style=\"color:black\"><span style=\"color:#0070c0\">如果要添加新的副本就需要去副本接口里添加新的副本代码也就是wdm文件，添加后再到这个副本ui文件里添加副本坐标。不然就会出现副本不显示或者副本不存在的问题。 </span></span></span></span></p>\n\n<p style=\"margin-left:41px\"><span style=\"font-size:14pt\"><span style=\"font-family:微软雅黑,sans-serif\"><span style=\"color:black\"><span style=\"color:#0070c0\">副本的提取和添加到这也就差不多了，</span><span style=\"color:red\">还有最重要的一点，提取的副本里面用搜索工具搜一下[NPC]词条，看看有没有你npc.lst里没有的npc，如果没有就将这个npc代码删除，不然添加副本以后进图去到有该npc代码的房间就闪退。 </span></span></span></span></p>\n\n<p style=\"margin-left:41px; margin-right:31px\"><span style=\"font-size:14pt\"><span style=\"font-family:微软雅黑,sans-serif\"><span style=\"color:black\">我们再来捋一遍思路：用工具提取城镇和副本，手动提取需要的 region和worldmap以及里面涉及到的rgn，mm；wdn，ui文件等。 </span></span></span></p>\n\n<p style=\"margin-left:41px; margin-right:31px\"><span style=\"font-size:14pt\"><span style=\"font-family:微软雅黑,sans-serif\"><span style=\"color:black\">城镇的rgn和mm的town index要对应，wdn添加副本，ui添加副本坐标。城镇区域添加互传坐标与光圈。副本文件去除没有的npc 代码。大致就这些吧，想到的和遇到的后面再补。 </span></span></span></p>\n\n<p style=\"margin-left:41px; margin-right:31px\"><span style=\"font-size:14pt\"><span style=\"font-family:微软雅黑,sans-serif\"><span style=\"color:black\">如果遇到赛利亚房间没有npc的情况的话，检查一下你的赛利亚房间一般都是0号区域里的NPC代码和坐标位置。如： </span></span></span></p>\n\n<p style=\"margin-left:-62px; margin-right:-10px\">&nbsp;</p>\n\n<table style=\"width:100%\">\n\t<tbody>\n\t\t<tr>\n\t\t\t<td>\n\t\t\t<div>\n\t\t\t<p>&nbsp;</p>\n\t\t\t</div>\n\t\t\t</td>\n\t\t</tr>\n\t</tbody>\n</table>\n\n<table style=\"width:100%\">\n\t<tbody>\n\t\t<tr>\n\t\t\t<td>\n\t\t\t<div>\n\t\t\t<p><span style=\"font-size:10.5pt\"><span style=\"color:red\">NPC</span></span> <span style=\"font-size:10.5pt\"><span style=\"color:red\">代码</span></span></p>\n\t\t\t</div>\n\t\t\t</td>\n\t\t</tr>\n\t</tbody>\n</table>\n\n<table style=\"width:100%\">\n\t<tbody>\n\t\t<tr>\n\t\t\t<td>\n\t\t\t<div>\n\t\t\t<p><span style=\"font-size:10.5pt\"><span style=\"color:red\">NPC</span></span> <span style=\"font-size:10.5pt\"><span style=\"color:red\">坐标</span></span></p>\n\t\t\t</div>\n\t\t\t</td>\n\t\t</tr>\n\t</tbody>\n</table>\n\n<table style=\"width:100%\">\n\t<tbody>\n\t\t<tr>\n\t\t\t<td>\n\t\t\t<div>\n\t\t\t<p><span style=\"font-size:10.5pt\"><span style=\"color:red\">里面的东西最好删掉问题最多出在这 </span></span></p>\n\t\t\t</div>\n\t\t\t</td>\n\t\t</tr>\n\t</tbody>\n</table>\n\n<p>&nbsp;</p>\n\n<p style=\"margin-left:79px; text-align:justify\"><span style=\"font-size:14pt\"><span style=\"font-family:微软雅黑,sans-serif\"><span style=\"color:black\"><span style=\"color:#0070c0\">[guild agit entrance info] </span></span></span></span></p>\n\n<p style=\"margin-left:79px; text-align:justify\"><span style=\"font-size:14pt\"><span style=\"font-family:微软雅黑,sans-serif\"><span style=\"color:black\"><span style=\"color:#0070c0\">[/guild agit entrance info] </span></span></span></span></p>\n\n<p style=\"margin-left:79px\"><span style=\"font-size:14pt\"><span style=\"font-family:微软雅黑,sans-serif\"><span style=\"color:black\">&nbsp;</span></span></span></p>\n\n<p style=\"margin-left:79px; text-align:justify\"><span style=\"font-size:14pt\"><span style=\"font-family:微软雅黑,sans-serif\"><span style=\"color:black\"><span style=\"color:#0070c0\">[pvp channel limit npc] </span></span></span></span></p>\n\n<p style=\"margin-left:79px; text-align:justify\"><span style=\"font-size:14pt\"><span style=\"font-family:微软雅黑,sans-serif\"><span style=\"color:black\"><span style=\"color:#0070c0\">[/pvp channel limit npc] </span></span></span></span></p>\n\n<p style=\"margin-left:1px; margin-right:27px; text-align:right\"><span style=\"font-size:14pt\"><span style=\"font-family:微软雅黑,sans-serif\"><span style=\"color:black\">这两串词条里面最好不带东西，用pvf文件内容搜索这两个词条，</span></span></span></p>\n\n<p style=\"margin-left:41px; margin-right:31px\"><span style=\"font-size:14pt\"><span style=\"font-family:微软雅黑,sans-serif\"><span style=\"color:black\">批处理删掉就行。害怕出问题就在批处理回去个空的俩词条。 </span></span></span></p>\n",
